{
  "format_version": "1.8.0",
  "animations": {
    "animation.assy_zombie_wolf1.look_at_target": {
      "loop": true,
      "bones": {
        "head": {
          "relative_to": {
            "rotation": "entity"
          },
          "rotation": [
            "query.target_x_rotation - this",
            "query.target_y_rotation - this",
            0
          ]
        }
      }
    },
    "animation.assy_zombie_wolf1.angry": {
      "loop": true,
      "bones": {
        "tail": {
          "rotation": [
            0,
            "query.is_angry ? -this : (math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed * 80.22 - this)",
            0
          ]
        }
      }
    },
    "animation.assy_zombie_wolf1.baby_scaling": {
      "loop": true,
      "bones": {
        "head": {
          "position": [
            0,
            1,
            -2
          ],
          "scale": [
            1.6,
            1.6,
            1.6
          ]
        }
      }
    },
    "animation.assy_zombie_wolf1.head_rot_z": {
      "loop": true,
      "bones": {
        "head": {
          "rotation": [
            0,
            0,
            "(query.is_interested ? (query.head_roll_angle * 57.3) : 0) + (query.is_shaking_wetness ? variable.head_rot_z : 0) - this"
          ]
        }
      }
    },
    "animation.assy_zombie_wolf1.leg_default": {
      "loop": true,
      "bones": {
        "leg0": {
          "rotation": [
            "math.cos(query.modified_distance_moved * 38.17) * 80.22 * query.modified_move_speed - this",
            0,
            0
          ]
        },
        "leg1": {
          "rotation": [
            "math.cos(query.modified_distance_moved * 38.17 + 180) * 80.22 * query.modified_move_speed - this",
            0,
            0
          ]
        },
        "leg2": {
          "rotation": [
            "math.cos(query.modified_distance_moved * 38.17 + 180) * 80.22 * query.modified_move_speed - this",
            0,
            0
          ]
        },
        "leg3": {
          "rotation": [
            "math.cos(query.modified_distance_moved * 38.17) * 80.22 * query.modified_move_speed - this",
            0,
            0
          ]
        }
      }
    },
